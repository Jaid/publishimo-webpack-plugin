  "license": "MIT",
  "author": {
    "name": "Jaid",
    "url": "https://github.com/jaid",
    "email": "jaid.jsx@gmail.com"
  },
  "repository": "https://github.com/Jaid/publishimo-webpack-plugin",
  "description": "Webpack plugin for publishimo",
  "main": "dist/index.js",
  "babel": {
    "presets": [
      "jaid"
    ]
  },
  "eslintConfig": {
    "extends": "jaid"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "publishimo": "^4.0.1",
    "stringify-author": "^0.1.3"
  },
  "devDependencies": {
    "babel-jest": "24.1.0",
    "babel-preset-jaid": "2.9.0",
    "clean-webpack-plugin": "^1.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-jaid": "1.11.0",
    "jest": "24.1.0",
    "load-json-file": "^5.1.0",
    "opn-cli": "^4.0.0",
    "pify": "^4.0.1",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3",
    "webpack-config-jaid": "^1.24.2"
  },
  "scripts": {
    "clean": "rimraf dist dist-jsdoc",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:dev": "webpack",
    "test": "jest && npm run build:prod && cross-env MAIN=dist jest",
    "test:dev": "npm run build:dev && jest",
    "test:debug": "node --inspect=9229 node_modules/jest/bin/jest.js --runInBand",
    "release": "npm test && cross-env git tag v$npm_package_version && git push --tags && cross-env opn https://github.com/$npm_package_author_name/$(basename $PWD)/releases/new?tag=v$npm_package_version"
  }
}
